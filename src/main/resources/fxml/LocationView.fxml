<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:id="view" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <VBox spacing="5.0" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </BorderPane.margin>
         <children>
            <GridPane hgap="10.0" vgap="10.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER" />
              </rowConstraints>
               <children>
                  <VBox alignment="CENTER_LEFT" fillWidth="false">
                     <children>
                        <Label style="-fx-font-weight: bold;" text="LATITUDE" />
                        <Label fx:id="latitudeLabel" text="Label" />
                     </children>
                     <GridPane.margin>
                        <Insets />
                     </GridPane.margin>
                  </VBox>
                  <VBox alignment="CENTER_LEFT" fillWidth="false" GridPane.columnIndex="1">
                     <GridPane.margin>
                        <Insets />
                     </GridPane.margin>
                     <children>
                        <Label style="-fx-font-weight: bold;" text="LONGITUDE" />
                        <Label fx:id="longitudeLabel" text="Label" />
                     </children>
                  </VBox>
                  <HBox alignment="CENTER_LEFT" spacing="5.0" GridPane.columnSpan="2" GridPane.rowIndex="1">
                     <GridPane.margin>
                        <Insets />
                     </GridPane.margin>
                     <children>
                        <Label style="-fx-font-weight: bold;" text="IP LOCATION:" />
                        <Label fx:id="ipLocationLabel" text="Label" />
                        <ImageView fx:id="flagImage" fitWidth="32.0" pickOnBounds="true" preserveRatio="true" />
                     </children>
                  </HBox>
                  <VBox alignment="CENTER_LEFT" fillWidth="false" GridPane.rowIndex="2">
                     <GridPane.margin>
                        <Insets />
                     </GridPane.margin>
                     <children>
                        <Label style="-fx-font-weight: bold;" text="CITY, STATE" />
                        <Label fx:id="cityStateLabel" text="Label" />
                     </children>
                  </VBox>
                  <VBox alignment="CENTER_LEFT" fillWidth="false" GridPane.columnIndex="1" GridPane.rowIndex="2">
                     <GridPane.margin>
                        <Insets />
                     </GridPane.margin>
                     <children>
                        <Label style="-fx-font-weight: bold;" text="ZIP CODE" />
                        <Label fx:id="zipCodeLabel" text="Label" />
                     </children>
                  </VBox>
                  <VBox alignment="CENTER_LEFT" fillWidth="false" GridPane.rowIndex="3">
                     <GridPane.margin>
                        <Insets />
                     </GridPane.margin>
                     <children>
                        <Label style="-fx-font-weight: bold;" text="LANGUAGE" />
                        <Label fx:id="languageLabel" text="Label" />
                     </children>
                  </VBox>
                  <VBox alignment="CENTER_LEFT" fillWidth="false" GridPane.columnIndex="1" GridPane.rowIndex="3">
                     <GridPane.margin>
                        <Insets />
                     </GridPane.margin>
                     <children>
                        <Label style="-fx-font-weight: bold;" text="TIME ZONE" />
                        <Label fx:id="timeZoneLabel" text="Label" />
                     </children>
                  </VBox>
                  <VBox alignment="CENTER_LEFT" fillWidth="false" GridPane.rowIndex="4">
                     <GridPane.margin>
                        <Insets />
                     </GridPane.margin>
                     <children>
                        <Label style="-fx-font-weight: bold;" text="CALLING CODE" />
                        <Label fx:id="callingCodeLabel" text="Label" />
                     </children>
                  </VBox>
                  <VBox alignment="CENTER_LEFT" fillWidth="false" GridPane.columnIndex="1" GridPane.rowIndex="4">
                     <GridPane.margin>
                        <Insets />
                     </GridPane.margin>
                     <children>
                        <Label style="-fx-font-weight: bold;" text="CURRENCY" />
                        <Label fx:id="currencyLabel" text="Label" />
                     </children>
                  </VBox>
               </children>
            </GridPane>
         </children>
      </VBox>
   </center>
</BorderPane>
